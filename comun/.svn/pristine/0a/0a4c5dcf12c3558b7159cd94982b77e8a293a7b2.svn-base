body {
	background-image: url(../img/fondo.jpg); background-repeat: repeat
}

/*#fondo_index { background-image: url(../img/clean.jpg); background-repeat: repeat }*/



header { float: left; width: 100%; padding-top: 20px; text-align: center}
	
	header div#menu_horizontal_superior {
		position: relative;
		height: 50px;
		
		background-color: #FFFFFF;
		border-bottom: 5px solid #333333;
	}
	
	header div#menu_horizontal_medio {
		background-color: transparent;
		height: 191px;
	}
	
	header div#menu_horizontal_inferior {
		position: relative;
		height: 60px;
		padding-top: 15px;
		
		background-color: #FFFFFF;
		border-top: 5px solid #333333;
	}
	
	header div#menu_horizontal_inferior ul li#carga_ajax {
		letter-spacing: 1px;
	}
	
	header ul#suscripcion {
		font-size: 18px;
		font-weight: bold;
		text-align: center;
	}
	
	header ul {
		text-align: right;
	}
	
	header ul li {
		display: inline;
	}

	header ul#social {
		text-align: left;
		position: relative;
	}
	
	header ul#social li {
		margin: 5px;
	}
	
	header ul#login {
		position: absolute;
		top: 15px;
		right: 10px;
		height: 20px;
	}

	header ul#login li {
		text-align: center;
		background-color: #FFFFFF;
		cursor: pointer;
	}
	
	header ul#login li a {
		text-decoration: none;
		font-size: 16px;
		color: #222;
		border-radius:7px;
		padding:5px;
	}
	
	header h1 { 
		font-size: 30px;
		text-align: center
	}
	
	header hr {
		margin-top: 20px;
	}
	
	header div#menu_horizontal_superior ul#login li a{
		background: #f4b61b;
		border:#222222 1px solid;
	}
	
	header div#menu_horizontal_superior ul#login li a:hover{
		background: #222;
		color:#F4B61B;
		border:#F4B61B 1px solid;
	}

div#bundle_actual { position: relative; float: left; width: 100%; min-height: 500px; background-image: url(../img/clean.jpg); background-repeat: repeat }

	
	.titlediv{
		 font-family: 'AnuDawRegular';
		 font-weight: bold;
		 border-radius: 5px 5px 0px 0px;
		 background-color: #f4b61b;
	}
	

	div#bundle_actual h1{
		font-size: 30px;
		text-align: center;
		padding: 5px;
	}

	div#descripcion {
		float: left; width: 75%; padding-top: 50px; text-align: center
	}
	
	div#descripcion p {
		text-align: left;
		padding: 20px;
	}
	
	div#descripcion img {
		width: inherit;
		height: inherit;
	}
	
	nav {
		float: left; width: 20%; height: auto; padding-top: 50px
	}

	nav ul {
		text-align: left
	}
	
	nav ul li {
		padding: 10px;
		margin: 5px;
		border-left: 1px solid black;
		border-right: 1px solid black;
		background-color: #FFFFFF;
		cursor: pointer;
	}
	
	nav ul li:first-child {
		border-top: 1px solid black;
		border-radius: 7px 0 0 0;
	}
	nav ul li:last-child {
		border-bottom: 1px solid black;
		border-radius: 0 0 7px 0;
	}
	
	nav ul li a {
		color: #0000FF;
	}
	
	div#detalles {
		position: relative;
		float: left;
		width: 100%;
	}
	
	div#detalles p {
		text-align: left;
		padding: 20px;
		padding-top: 50px;
	}
	

div#estadisticas { position: relative; float: left; width: 100%; margin-top: 20px; min-height: 300px; background-color: #FFFFFF }

	div#estadisticas h1 {
		text-align: center;
		font-size: 30px;
		padding: 5px;
	}
	
	div#estadisticas p {
		text-align: left;
		padding: 20px;
	}

div#comprar { position: relative; float: left; width: 100%; margin-top: 20px; min-height: 600px; background-color: #FFF26F }

div#comprar h1 {
		text-align: center;
		font-size: 30px;
		padding: 5px;
	}
	
	div#comprar p {
		text-align: left;
		padding: 20px;
	}


footer { float: left; width: 100%; padding-top: 50px; padding-bottom: 20px }

	footer hr {
		margin-bottom: 20px;
		background-color: #FFFFFF
	}

	footer p {
		text-align: center;
		background-color: #FFFFFF
	}
