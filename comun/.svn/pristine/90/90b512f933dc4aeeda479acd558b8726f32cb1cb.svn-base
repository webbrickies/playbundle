/* CSS Reset */

html, body, div, span, h1, h2, h3, h4, h5, h6, p, img, ul, li, fieldset, form, label { margin: 0; padding: 0; border: 0; outline: 0; font-size: 100%; vertical-align: baseline; background: transparent; }
ol, ul { list-style: none; }

:focus { outline: 0; }

/* General */

body { background-color: #FFFFFF; font: 14px ArchitectsDaughterRegular; line-height: 1.6em; color: #000000; }

h1, h2, h3, h4, h5 { font-weight: normal; }

a { text-decoration: none; }

.main_container { margin: 0 auto; width: 960px }

.redondeado { border-radius: 5px; }

/* Fuentes */

@font-face {
    font-family: 'Roboto Slab', serif;
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'Denk One', sans-serif;
    font-weight: normal;
    font-style: normal;
}

